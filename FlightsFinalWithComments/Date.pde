
/*
  This class will sort through the flights based on the dates entered by the
 user, either a date range such as flexible dates or specific dates for departure and returns.
 */
class Date {
  void draw()
  {

    if (dateGo)
    {
      background(200, 0, 0);
      fill(0, 200, 0);
      rect(100, 100, 200, 200);
    }
  }
}
